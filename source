--Made by Mods youtube Channel ExoticKiddo

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Weapon X Hub", "BloodTheme")

--PLAYER
local Main = Window:NewTab("Player")
local MainSection = Main:NewSection("Player")


MainSection:NewButton("Infinite Yeild", "FE Admin Commands", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

MainSection:NewButton("BackFlip", "Can backflip + frontflip by pressing a keybind that they tell you", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ExoticModifier/Tester/main/Flip",true))();
end)

MainSection:NewButton("Fly Custom Made", "you can fly by pressing the keybind (it gradually gets faster)", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ExoticModifier/Fly/main/Script",true))();
end)

MainSection:NewButton("Animations And Emotes", "Anyone Can See The Emotes And Animations", function()
    loadstring(game:HttpGet(loadstring(game:HttpGet("https://gitlab.com/Tsuniox/lua-stuff/-/raw/master/R15GUI.lua"))(),true))();
end)


